<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiFirstUartApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiFirstUartApp"
    xmlns:views="clr-namespace:MauiFirstUartApp.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="홈 홈홈"
            ContentTemplate="{DataTemplate local:MainPage}"
            Icon="appicon.png" />
        <ShellContent
            Title="Empty"
            ContentTemplate="{DataTemplate local:MainPage}"
            Icon="dotnet_bot.png" />
        <ShellContent
            Title="설정"
            ContentTemplate="{DataTemplate views:SettingPage}"
            Icon="appicon.png" />
        
        
    </TabBar>
</Shell>
